{
    "name": "speicher210/monsum-bundle",
    "type": "library",
    "description": "Symfony Bundle for Monsum API.",
    "keywords": [
        "monsum",
        "fastbill",
        "bundle"
    ],
    "homepage": "https://github.com/speicher210/monsum-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dragos Protung",
            "email": "dragos@protung.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "speicher210/monsum-api": "^1.0, >=1.0.5",
        "symfony/framework-bundle": "^2.5|^3.0",
        "symfony/validator": "^2.5|^3.0",
        "symfony/console": "^2.5|^3.0",
        "jms/serializer-bundle": "^1.1|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7",
        "symfony/browser-kit": "^2.5|^3.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Speicher210\\MonsumBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Speicher210\\MonsumBundle\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "cs-check": "phpcs --warning-severity=0",
        "cs-fix": "phpcbf -p"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
